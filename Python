def get_CONSERVE_value(CONSERVE):
    if CONSERVE is None:
        return None
    CONSERVE = CONSERVE.upper()
    
    if "A" in CONSERVE:
        return "Excellente"
    elif "B" in CONSERVE:
        return "Bonne"
    elif "C" in CONSERVE:
        return "Moyenne/réduite"
    elif "C" in CONSERVE:
        return "Significative"
    else:
        return "Non défini"